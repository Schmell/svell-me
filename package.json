{
	"name": "svell-me",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "prisma generate && vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"reset": "npx prisma migrate reset",
		"again": " npx prisma migrate dev --name again"
	},
	"devDependencies": {
		"@faker-js/faker": "^7.6.0",
		"@iconify/svelte": "^3.1.0",
		"@melt-ui/pp": "^0.0.5",
		"@playwright/test": "^1.30.0",
		"@skeletonlabs/skeleton": "^1.2.5",
		"@sveltejs/adapter-auto": "^1.0.2",
		"@sveltejs/adapter-vercel": "^3.0.1",
		"@sveltejs/kit": "^1.3.10",
		"@types/papaparse": "^5.3.7",
		"@typescript-eslint/eslint-plugin": "^5.50.0",
		"@typescript-eslint/parser": "^5.50.0",
		"autoprefixer": "^10.4.7",
		"eslint": "^8.33.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"postcss": "^8.4.14",
		"postcss-load-config": "^4.0.1",
		"prettier": "^2.8.3",
		"prettier-plugin-svelte": "^2.9.0",
		"prisma": "^5.0.0",
		"rome": "^11.0.0",
		"svelte": "^3.55.1",
		"svelte-check": "^3.0.3",
		"svelte-preprocess": "^5.0.3",
		"svelte-sequential-preprocessor": "^2.0.1",
		"sveltekit-superforms": "^0.7.0",
		"tailwindcss": "^3.2.7",
		"tslib": "^2.5.0",
		"typescript": "^4.9.5",
		"vite": "^4.1.1",
		"vitest": "^0.25.8",
		"zod": "^3.21.4"
	},
	"type": "module",
	"dependencies": {
		"@dicebear/collection": "^5.3.4",
		"@dicebear/core": "^5.3.4",
		"@lucia-auth/adapter-prisma": "^0.4.0",
		"@lucia-auth/sveltekit": "^0.6.2",
		"@melt-ui/svelte": "^0.24.0",
		"@paralleldrive/cuid2": "^2.2.0",
		"@prisma/client": "^5.0.0",
		"@tanstack/svelte-table": "^8.8.5",
		"@vercel/postgres": "^0.4.0",
		"daisyui": "^2.50.2",
		"lucia": "^1.0.0",
		"lucia-auth": "^0.6.2",
		"papaparse": "^5.3.2"
	}
}
